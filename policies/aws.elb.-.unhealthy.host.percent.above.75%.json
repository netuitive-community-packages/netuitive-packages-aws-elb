{
  "policy": {
    "actions": [
      {
        "category": 3,
        "type": "event"
      }
    ],
    "conditions": [
      {
        "analytic": "actual",
        "level": 75,
        "metric": "netuitive.aws.elb.unhealthyhostpercent",
        "operator": ">"
      }
    ],
    "deleted": false,
    "description": "More than three quarters (75%) of the hosts associated with this ELB are in an unhealthy state.",
    "duration": 300,
    "enabled": false,
    "name": "AWS ELB - Unhealthy Host Percent Above 75%",
    "scope": {
      "elementTypes": [
        "ELB"
      ]
    }
  }
}