{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "aws.elb.requestcount",
        "properties": {
          "STATISTIC": "sum",
          "sparseDataStrategy": "ReplaceWithZero"
        }
      },
      {
        "match": "aws.elb.latency",
        "properties": {
          "sparseDataStrategy": "ReplaceWithZero",
          "tags": {
            "unit": "s"
          }
        }
      },
      {
        "match": "aws.elb.httpcode.*",
        "properties": {
          "STATISTIC": "sum",
          "sparseDataStrategy": "ReplaceWithZero"
        }
      },
      {
        "match": "aws.elb.backendconnectionerrors",
        "properties": {
          "STATISTIC": "sum"
        }
      },
      {
        "match": "aws.elb.spillovercount",
        "properties": {
          "STATISTIC": "sum",
          "sparseDataStrategy": "ReplaceWithZero"
        }
      },
      {
        "match": "netuitive.aws.elb.surgequeueutilization",
        "properties": {
          "tags": {
            "unit": "percent",
            "utilization": true
          },
          "validMax": 100
        }
      },
      {
        "match": "aws.elb\\..*healthyhostcount",
        "properties": {
          "sparseDataStrategy": "ReplaceWithLast"
        }
      },
      {
        "match": "aws.elb.backendconnectionerrors",
        "properties": {
          "sparseDataStrategy": "ReplaceWithZero"
        }
      },
      {
        "match": "aws.elb.surgequeuelength.*",
        "properties": {
          "sparseDataStrategy": "ReplaceWithZero",
          "validMax": 1024
        }
      },
      {
        "match": ".*aws.elb\\..*percent.*",
        "properties": {
          "tags": {
            "unit": "percent"
          },
          "validMax": 100
        }
      },
      {
        "match": "netuitive.aws.elb.requestspersecond",
        "properties": {
          "tags": {
            "unit": "ops"
          }
        }
      }
    ],
    "name": "AWS ELB",
    "scope": {
      "elementType": "ELB"
    },
    "type": "METRIC_META"
  }
}
